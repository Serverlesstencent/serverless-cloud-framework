{
  "name": "serverless-cloud-framework",
  "version": "1.3.0-beta.1",
  "description": "Serverless Tencent command line tool",
  "main": "./dist/index.js",
  "author": "Tencent Cloud, Inc.",
  "bin": {
    "scf": "./bin/serverless-cloud-framework",
    "serverless-cloud-framework": "./bin/serverless-cloud-framework"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:updated": "pipe-git-updated --ext=js -- eslint",
    "prettier-check:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml -- prettier -c",
    "prebuild": "rm -fr dist",
    "build": "webpack",
    "prettify": "prettier --write src/ tests/ scripts/ .github/workflows/ CHANGELOG.md",
    "test": "jest ./tests --verbose -o",
    "test:coverage": "jest --silent --coverage --coverageReporters=\"text\" --coverageReporters=\"text-summary\"",
    "testOne": "jest --verbose -o",
    "prepare": "husky install",
    "pkg:build": "node ./scripts/pkg/build.js",
    "pkg:upload": "node ./scripts/pkg/upload/index.js",
    "warmCDN": "node ./scripts/cdnWarmer/index.js",
    "release": "semantic-release",
    "scan": "i18next-scanner --config i18n/config"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "dev",
        "channel": "beta",
        "prerelease": "beta"
      },
      "next"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/release-notes-generator",
        {
          "linkCompare": false,
          "linkReferences": false
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@tencent/semantic-release-tgit-z",
        {
          "assets": [
            {
              "path": "dist/*",
              "label": "npm distribution"
            }
          ]
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npm run build"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@serverless/eslint-config/node",
    "root": true,
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": "off"
    }
  },
  "eslintIgnore": [
    "!/.github",
    "webpack.config.js"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{css,html,js,json,md,yaml,yml}": [
      "prettier -c"
    ]
  },
  "dependencies": {
    "@serverless-cloud-framework/platform-client-china": "^1.0.4",
    "@serverless/utils": "5.20.3",
    "adm-zip": "^0.5.4",
    "ansi-escapes": "^4.3.1",
    "chalk": "^4.1.0",
    "child-process-ext": "^2.1.1",
    "chokidar": "^3.5.1",
    "ci-info": "^3.2.0",
    "cli-progress-footer": "^2.3.0",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "fastest-levenshtein": "^1.0.12",
    "figures": "^3.2.0",
    "fs-extra": "^9.1.0",
    "got": "^11.8.2",
    "graphlib": "^2.1.8",
    "hash-string": "^1.0.0",
    "https-proxy-agent": "^5.0.0",
    "i18next": "^23.15.1",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "js-yaml": "^4.0.0",
    "memoizee": "^0.4.14",
    "minimist": "^1.2.5",
    "prettyoutput": "^1.2.0",
    "ramda": "^0.27.1",
    "semver": "^7.3.4",
    "strip-ansi": "^6.0.0",
    "tencent-serverless-http": "^1.3.1",
    "traverse": "^0.6.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "9.0.0",
    "@serverless/eslint-config": "^4.0.0",
    "@tencent/semantic-release-tgit-z": "^1.2.3",
    "cos-nodejs-sdk-v5": "^2.14.2",
    "eslint": "^8.0.0",
    "eslint-plugin-i18next": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "essentials": "^1.1.1",
    "git-list-updated": "^1.2.1",
    "github-release-from-cc-changelog": "^2.2.0",
    "husky": "^7.0.0",
    "i18next-scanner": "^4.5.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.2",
    "pkg": "^5.5.1",
    "prettier": "^2.4.1",
    "process-utils": "^4.0.0",
    "semantic-release": "^17.4.7",
    "tencentcloud-sdk-nodejs": "^4.0.301",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=12.0"
  },
  "license": "MIT"
}
